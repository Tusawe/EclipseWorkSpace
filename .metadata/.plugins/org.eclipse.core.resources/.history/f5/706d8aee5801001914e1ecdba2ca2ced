package cafetera;

public class Consumible{
	
	private int tiempo;
	private double precio;
	private double agua;
	private int capsulas;

	public Consumible( int tiempo, double precio, double agua, int capsulas) {
		
		this.tiempo = tiempo;
		this.precio = precio;
		this.agua = agua;
		this.capsulas = capsulas;
	
	}

}
